<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="Sistema_de_Cambios_de_Carrera" extends="struts-default">      
        
        <action name="buscarEst" class="actions.EstudianteAction" method="buscarEstudiante"> 
            <result name="estudiante" type="redirect">/FormularioSolicitud.jsp</result> 
            <result name="admin" type="redirect">/GestionarCatalogos.jsp</result>
            <result name="no success" type="redirect">/ErrorAuth.jsp</result>
        </action>
       
       
        <action name="verificarSol" class="actions.SolicitudAction" method="mostrarSolicitud"> 
            <result name="success">/AgregadaSol.jsp</result> 
            <result name="no success">/MalaSol.jsp</result> 
        </action> 
    </package>
</struts>